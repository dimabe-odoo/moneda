<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="base.CLP" model="res.currency">
        <field name="name">CLP</field>
        <field name="symbol">CLP</field>
        <field name="rounding">1.00</field>
        <field name="active" eval="True"/>
        <field name="position">before</field>
        <field name="currency_unit_label">Peso</field>
        <field name="currency_subunit_label">Centavos</field>
        <field name="rate_inverted" eval="True"/>
    </record>
    <record id="base.USD" model="res.currency">
        <field name="symbol">USD </field>
        <field name="position">before</field>
    </record>

  </data>
  <data>
    <!--<record id="l10n_cl_currency_rate_update.UF" model="res.currency">-->
    <record id="UF" model="res.currency">
        <field name="name">UF</field>
        <field name="symbol">UF </field>
        <field name="rounding">0.01</field>
        <field name="active" eval="True"/>
        <field name="position">before</field>
        <field name="currency_unit_label">Unidades de Fomento</field>
        <field name="currency_subunit_label">Céntimos de UF</field>
    </record>
    <record id="UTM" model="res.currency">
        <field name="name">UTM</field>
        <field name="symbol">UTM </field>
        <field name="rounding">0.01</field>
        <field name="active" eval="True"/>
        <field name="position">before</field>
        <field name="currency_unit_label">Unidades Tributarias Mensuales</field>
        <field name="currency_subunit_label">Céntimos de UTM</field>
    </record>
    <record id="UTA" model="res.currency">
        <field name="name">UTA</field>
        <field name="symbol">UTA </field>
        <field name="rounding">0.01</field>
        <field name="active" eval="True"/>
        <field name="position">before</field>
        <field name="currency_unit_label">Unidades Tributarias Anuales</field>
        <field name="currency_subunit_label">Céntimos de UTA</field>
    </record>
  </data>

</odoo>
